{
  "[dart]": {
    "cSpell.fixSpellingWithRenameProvider": true,
    "editor.formatOnSave": false,
    "editor.formatOnType": false,
    "editor.lightbulb.enabled": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.preview": true,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "on",
    "editor.wordBasedSuggestions": true,
    "editor.wordBasedSuggestionsMode": "allDocuments"
  },
  "cSpell.words": [
    "Bazel",
    "Bodymovin",
    "cami",
    "camilab",
    "ccpdt",
    "CCSI",
    "DABI",
    "DACI",
    "DCSI",
    "Dhiwise",
    "DOBI",
    "DOPI",
    "DPAI",
    "kakaolink",
    "Knollo",
    "kpostal",
    "MBTI",
    "naver",
    "perano",
    "rxdart",
    "shalimar",
    "trivy",
    "zumthor"
  ],
  "dart.autoImportCompletions": true,
  "dart.buildRunnerAdditionalArgs": ["--delete-conflicting-outputs"],
  "dart.checkForSdkUpdates": true,
  "dart.devToolsBrowser": "default",
  "dart.devToolsReuseWindows": true,
  "dart.documentation": "full",
  "dart.enableSdkFormatter": true,
  "dart.flutterCreateAndroidLanguage": "kotlin",
  "dart.flutterCreateIOSLanguage": "swift",
  "dart.flutterCreatePlatforms": ["android", "ios", "macos", "web"],
  "dart.flutterGutterIcons": true,
  "dart.flutterHotReloadOnSave": "all",
  "dart.flutterSelectDeviceWhenConnected": true,
  "dart.flutterWebRenderer": "auto",
  "dart.openDevTools": "flutter",
  "dart.previewFlutterUiGuides": true,
  "dart.promptToRunIfErrors": true,
  "dart.renameFilesWithClasses": "prompt",
  "dart.showDartPadSampleCodeLens": true,
  "dart.showMainCodeLens": true,
  "dart.showTodos": true,
  "dart.updateDevTools": true,
  "dart.updateImportsOnRename": true,
  "dart.warnWhenEditingFilesInPubCache": true,
  "jsonToDart.codeGenerator": "Freezed",
  "jsonToDart.copyWith": true,
  "jsonToDart.equality": "Equatable",
  "jsonToDart.fastMode": true,
  "jsonToDart.immutable": true,
  "jsonToDart.json.codecs": true,
  "jsonToDart.nullSafety": true,
  "jsonToDart.primaryConfiguration": true,
  "jsonToDart.runBuilder": true,
  "jsonToDart.sortConstructorsFirst": true,
  "jsonToDart.targetDirectory.path": "/lib/data/",
  "jsonToDart.toString": "Auto",
  "svg.preview.background": "transparent"
}
