{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "cami_lab",
      "request": "launch",
      "type": "dart",
      "flutterPlatform": "default",
      "flutterMode": "debug",
      "console": "debugConsole",
      "args": [
        "--hot",
        "--ignore-deprecation",
        "--route=/app_navigation_screen"
      ],
      "openDevTools": "logging",
      "program": "lib/main.dart",
      "suppressWebServerDeviceBrowserLaunch": true
    },
    {
      "name": "cami_lab (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterPlatform": "default",
      "flutterMode": "profile",
      "console": "debugConsole",
      "args": ["--hot", "--ignore-deprecation"],
      "openDevTools": "performance",
      "program": "lib/main.dart"
    },
    {
      "name": "cami_lab (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterPlatform": "default",
      "flutterMode": "release",
      "console": "debugConsole",
      "args": ["--hot", "--ignore-deprecation"],
      "openDevTools": "logging",
      "program": "lib/main.dart"
    }
  ]
}
